dnsmasq_public_dns: False
dnsmasq_dhcpv4: True
dnsmasq_dhcpv6: False
dnsmasq_router: False
dnsmasq_tftp: False
dnsmasq_upstream_interfaces: [ 'eth1' ]
dnsmasq_public_dns: False
dnsmasq_ipv4_gateway: 'eth1'
dnsmasq_ipv6_gateway: ""
dnsmasq_interfaces:
  - interface: 'eth0'
    name: 'gw'
    aliases: [ 'router', 'gateway' ]
    dhcp_range_start: '2'
    dhcp_range_end: '-106'
    dhcp_lease: '12h'
dnsmasq_domain: ''
dnsmasq_domain_mx: False
dnsmasq_etc_hosts: False
dnsmasq_search: False
dnsmasq_static_dhcp_hosts:
  - macs:
      - 00:01:02:03:04:05
      - 01:02:03:04:05:06
    ip: 192.168.1.3

dnsmasq_options: 'bogus-nxdomain=64.94.110.11\nlog-queries\ndhcp-authoritative'
